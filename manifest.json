{
  "manifest_version": 3,
  "name": "Save Rendered HTML",
  "version": "1.1.7",
  "description": "A Chrome extension that saves the fully rendered HTML of the current tab.",
  "permissions": ["activeTab", "downloads", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png"
    }
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
